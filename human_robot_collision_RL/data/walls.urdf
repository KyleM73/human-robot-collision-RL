<?xml version="1.0"?>
<robot name="walls">
  <link name="back_wall">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="100"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <box size="2.0 0.1 2.0"/>
      </geometry>
      <material name="concrete">
        <color rgba="0.5803921569 0.5803921569 0.5803921569 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      <geometry>
        <box size="2.0 0.1 2.0"/>
      </geometry>
    </collision>
  </link>

  <link name="right_wall">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="100"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.1 30.0 2.0"/>
      </geometry>
      <material name="concrete">
        <color rgba="0.5803921569 0.5803921569 0.5803921569 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.1 30.0 2.0"/>
      </geometry>
    </collision>
  </link>

  <joint name="back_to_right_wall" type="fixed">
    <parent link="back_wall"/>
    <child link="right_wall"/>
    <origin xyz="0.95 14.95 0.0"/>
  </joint>

    <link name="left_wall">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="100"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.1 30.0 2.0"/>
      </geometry>
      <material name="concrete">
        <color rgba="0.5803921569 0.5803921569 0.5803921569 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.1 30.0 2.0"/>
      </geometry>
    </collision>
  </link>

  <joint name="back_to_left_wall" type="fixed">
    <parent link="back_wall"/>
    <child link="left_wall"/>
    <origin xyz="-0.95 14.95 0.0"/>
  </joint>

</robot>